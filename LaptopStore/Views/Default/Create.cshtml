@model Laptop.Entity.LaptopSpecification

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>


@using (Html.BeginForm("Create","Index")) 
{
<table border="1">
    <tr>
        <td>ID :</td>
        <td> @Html.TextBoxFor(Model => Model.Id)</td>
    </tr>
    <tr>
        <td>Laptop Brand : </td>
        <td>@Html.TextBoxFor(Model => Model.Brand)</td>
    </tr>
    <tr>
        <td>Price : </td>
        <td>@Html.TextBoxFor(Model => Model.Price)</td>
    </tr>
    <tr>
        <td>Description: </td>
        <td>@Html.TextBoxFor(Model => Model.Description)</td>
    </tr>
   
    <tr>
        <td>
            <input type="submit" />
        </td>
        <td><a href="/Index">Back</a></td>
    </tr>
</table>
}

